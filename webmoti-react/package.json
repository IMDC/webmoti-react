  "name": "webmoti-react",
  "private": true,
  "license": "Apache-2.0",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fireworks-js/react": "^2.10.8",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@twilio/conversations": "^2.6.2",
    "@twilio/video-processors": "^3.1.0",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "d3-timer": "^3.0.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase": "^11.8.1",
    "firebase-admin": "^13.4.0",
    "is-plain-object": "^5.0.0",
    "linkify-it": "^5.0.0",
    "livekit-client": "^2.13.3",
    "lodash.throttle": "^4.1.1",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hotkeys-hook": "^5.1.0",
    "react-router-dom": "^7.6.0",
    "react-speech-recognition": "^4.0.1",
    "react-use-websocket": "^4.13.0",
    "swiper": "^8.1.5",
    "ts-node": "^10.9.2",
    "twilio": "^5.6.1",
    "twilio-video": "^2.31.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3-timer": "^3.0.2",
    "@types/jest": "^29.5.14",
    "@types/linkify-it": "^5.0.0",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "^22.15.21",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@types/react-speech-recognition": "^3.9.6",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "^9.27.0",
    "eslint-plugin-import": "^2.31.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "postinstall": "rimraf public/virtualbackground && copyfiles -f node_modules/@twilio/video-processors/dist/build/* public/virtualbackground",
    "noisecancellation:krisp": "npm install @twilio/krisp-audio-plugin && rimraf public/noisecancellation && copyfiles -f \"node_modules/@twilio/krisp-audio-plugin/dist/*\" public/noisecancellation && copyfiles -f \"node_modules/@twilio/krisp-audio-plugin/dist/weights/*\" public/noisecancellation/weights",
    "start": "concurrently npm:server npm:dev",
    "dev": "vite",
    "build": "vite build",
    "test:base": "cross-env TZ=utc jest --config jest.config.js",
    "test": "npm run test:base",
    "test-specific": "npm run test:base -- --testPathPattern",
    "update-snapshot-specific": "npm run test:base -- -u --testPathPattern",
    "update-all-snapshots": "npm run test:base -- -u",
    "test:ci": "npm run test:base -- --ci --runInBand --reporters=default --reporters=jest-junit --coverage --silent",
    "lint": "eslint src server",
    "lint-fix-imports": "eslint --config .eslintrc_on_save.json --fix",
    "server": "ts-node -T -P server/tsconfig.json server/index.ts",
    "typescript:server": "tsc --noEmit -p server/",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "cypress:ci": "cross-env CYPRESS_baseUrl=http://localhost:8081 start-server-and-test server http://localhost:8081 cypress:run",
    "deploy:base": "npm run build && twilio rtc:apps:video:deploy --authentication=firebase --app-directory ./build --override",
    "deploy": "npm run deploy:base -- --room-type=group",
    "deploy-go": "npm run deploy:base -- --room-type=go",
    "deploy-debug": "npm run deploy:base -- --room-type=group -l debug",
    "view": "twilio rtc:apps:video:view",
    "delete": "twilio rtc:apps:video:delete"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
